{
  "name": "constellation-viewer",
  "version": "2.0.0",
  "description": "Interactive Star Map & Constellation Viewer with real-time astronomical calculations",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "dev": "python -m http.server 8000 --directory src",
    "build": "npm run build:css && npm run build:js",
    "build:css": "echo 'CSS build completed'",
    "build:js": "echo 'JS build completed'",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "serve": "python -m http.server 8000 --directory src",
    "start": "npm run serve"
  },
  "keywords": [
    "astronomy",
    "constellation",
    "star-map",
    "solar-system",
    "interactive",
    "visualization",
    "space",
    "stars",
    "planets"
  ],
  "author": "Constellation Viewer Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "babel-jest": "^30.1.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/constellation-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/constellation-viewer/issues"
  },
  "homepage": "https://github.com/your-username/constellation-viewer#readme"
}
